{
  "name": "elapsed-time",
  "version": "0.0.1",
  "description": "nanosecond resolution `stopwatch` for node.js",
  "keywords": [
    "elapsed",
    "stopwatch",
    "nanoseconds",
    "interval",
    "benchmark",
    "profile",
    "performance"
  ],
  "bugs": {
    "url": "https://github.com/fanatid/elapsed-time/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Kirill Fomichev",
    "email": "fanatid@ya.ru"
  },
  "files": [
    "lib",
    "src",
    "LICENSE",
    "README.md"
  ],
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/fanatid/elapsed-time"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run compile",
    "clean": "rimraf coverage distcheck lib",
    "compile": "babel src -d lib --optional runtime -s true",
    "coverage": "istanbul cover _mocha -- --compilers js:babel/register test/*.js",
    "coveralls": "npm run coverage && coveralls <coverage/lcov.info",
    "dist": "npm run clean && npm run compile && npm run distcheck",
    "distcheck": "npm run distcheck:prepare && npm run distcheck:test",
    "distcheck:prepare": "rimraf distcheck && node ./scripts/prepare-distcheck.js",
    "distcheck:test": "npm run distcheck:test:browser && npm run distcheck:test:node",
    "distcheck:test:browser": "cd distcheck/browser && karma start karma.conf.js",
    "distcheck:test:node": "mocha --compilers js:babel/register distcheck/node/test/*.js",
    "lint": "eslint scripts src test",
    "test": "npm run test:node && npm run test:browser",
    "test:browser": "karma start karma.conf.js",
    "test:node": "npm run test:node:babel",
    "test:node:babel": "mocha --compilers js:babel/register test/*.js",
    "test:node:skipasync": "cross-env TEST_SKIPASYNC=1 npm run test:node:babel",
    "watch:src": "mkdir -p lib && babel src -d lib --optional runtime -s true -w"
  },
  "dependencies": {
    "babel-runtime": "^5.8.20",
    "browser-process-hrtime": "^0.1.2"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.0",
    "babelify": "^6.2.0",
    "chai": "^3.2.0",
    "copy-dir": "^1.2.0",
    "coveralls": "^2.11.4",
    "cross-env": "^2.0.1",
    "eslint": "^1.2.1",
    "eslint-config-standard": "^4.1.0",
    "eslint-plugin-standard": "^1.3.0",
    "istanbul": "^0.3.18",
    "karma": "^1.7.1",
    "karma-browserify": "^4.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^0.2.0",
    "mocha": "^2.2.5",
    "puppeteer": "0.0.0",
    "rimraf": "^2.7.1"
  },
  "browser": {
    "./lib/hrtime.js": "./lib/hrtime-browser.js",
    "./src/hrtime.js": "./src/hrtime-browser.js"
  }
}
